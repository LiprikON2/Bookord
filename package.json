{
    "name": "bookord",
    "productName": "Bookord",
    "version": "0.2.4",
    "description": "E-book reader electron app",
    "main": ".webpack/main",
    "scripts": {
        "postinstall": "(cd src/forks && npm install)",
        "dev": "cross-env NODE_ENV=development electron-forge start",
        "//": "Packages into executable, then makes installer for it",
        "package": "electron-forge package && npm run beep",
        "make": "electron-forge make && npm run beep",
        "publish": "electron-forge publish",
        "lint": "eslint src/ --ext .ts,.js,.tsx,.jsx",
        "update": "npx npm-check-updates -i",
        "beep": "(cd tools/beep && node .)"
    },
    "keywords": [
        "electron",
        "epub",
        "ERWT"
    ],
    "author": {
        "name": "LiprikON2",
        "url": "https://github.com/LiprikON2"
    },
    "license": "MIT",
    "config": {
        "forge": "./tools/forge/forge.config.js"
    },
    "devDependencies": {
        "@electron-forge/cli": "^7.3.1",
        "@electron-forge/maker-deb": "7.3.1",
        "@electron-forge/maker-rpm": "7.3.1",
        "@electron-forge/maker-squirrel": "7.3.1",
        "@electron-forge/maker-zip": "7.3.1",
        "@electron-forge/plugin-webpack": "7.3.1",
        "@electron-forge/publisher-github": "^7.4.0",
        "@marshallofsound/webpack-asset-relocator-loader": "0.5.0",
        "@pmmmwh/react-refresh-webpack-plugin": "0.5.11",
        "@tanstack/eslint-plugin-query": "^5.17.7",
        "@types/node": "20.7.1",
        "@types/react": "18.2.23",
        "@types/react-dom": "18.2.8",
        "@types/webpack-env": "1.18.2",
        "@typescript-eslint/eslint-plugin": "6.7.3",
        "@typescript-eslint/parser": "6.7.3",
        "@vercel/webpack-asset-relocator-loader": "1.7.3",
        "copy-webpack-plugin": "^12.0.1",
        "cross-env": "7.0.3",
        "css-loader": "6.8.1",
        "electron": "29.3.0",
        "eslint": "8.50.0",
        "eslint-import-resolver-alias": "1.1.2",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-mobx": "^0.0.9",
